{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-local": "env-cmd -f .env gatsby build",
    "develop": "env-cmd -f .env gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.17.1",
    "bootstrap": "5.3.0",
    "fbjs": "^1.0.0",
    "flickity": "^2.2.1",
    "flickity-bg-lazyload": "^1.0.1",
    "gatsby": "5.12.3",
    "gatsby-plugin-compile-es6-packages": "2.1.1",
    "gatsby-plugin-manifest": "5.12.0",
    "gatsby-plugin-react-helmet": "6.12.0",
    "gatsby-plugin-sass": "6.12.0",
    "gatsby-plugin-sharp": "5.12.0",
    "gatsby-remark-images": "7.12.0",
    "gatsby-remark-relative-images": "2.0.5",
    "gatsby-source-contentful": "8.12.0",
    "gatsby-source-filesystem": "5.12.0",
    "gatsby-transformer-inline-svg": "1.2.0",
    "gatsby-transformer-remark": "6.12.0",
    "imagesloaded": "5.0.0",
    "mapbox-gl": "^1.9.1",
    "sass": "1.66.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "18.2.0",
    "react-bootstrap": "2.8.0",
    "react-dom": "18.2.0",
    "react-flickity-component": "4.0.6",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
